import discord
import random
questions=["Will you be leaving the house tonight?","How was the bathroom?","Watched any good movies lately?","Can I borrow that mouse?"]
intents = discord.Intents.default()
client = discord.Client(intents=intents)
intents.message_content = True
@client.event
async def on_ready():
    print(f'Logged in as {client.user}')

@client.event
async def on_message(message):
      if message.author == client.user:
        return
      else:  
        chance=random.randint(7,10)
        if chance==8:
            question=random.randint(0,3)
            print("question sent!")
            await message.channel.send(questions[question])
        print("message sent!")
        print (message.content)
  
